Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"RungeKutta", "[", 
    RowBox[{
    "phi_", ",", "t_", ",", "y_", ",", "t0_", ",", "y0_", ",", "intervall_", 
     ",", "steps_"}], "]"}], ":=", " ", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Input", " ", "vectors", " ", "must", " ", "be", " ", "the", " ", "same", 
     " ", "size"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "xList", ",", "yList", ",", "k1", ",", "k2", ",", "k3", ",", "k4", ",", 
       "ti", ",", "yi", ",", "out"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"yi", "=", "y0"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"out", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "0"}], ",", 
        RowBox[{"i", "\[LessEqual]", 
         RowBox[{"(", "steps", ")"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"ti", "=", 
          RowBox[{"t0", "+", 
           RowBox[{"intervall", "*", "i"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"k1", "=", 
          RowBox[{"phi", "/.", 
           RowBox[{"Flatten", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"t", "->", "ti"}], ",", 
              RowBox[{"Thread", "[", 
               RowBox[{"y", "->", "yi"}], "]"}]}], "}"}], "]"}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"k2", "=", 
          RowBox[{"phi", "/.", 
