                RowBox[{"(", 
                 RowBox[{"intervall", "/", "2"}], ")"}]}]}], ",", 
              RowBox[{"Thread", "[", 
               RowBox[{"y", "->", 
                RowBox[{"yi", "+", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"intervall", "/", "2"}], ")"}], "*", "k1"}]}]}], 
               "]"}]}], "}"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"k3", "=", 
          RowBox[{"phi", "/.", 
           RowBox[{"Flatten", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"t", "->", 
               RowBox[{"ti", "+", 
                RowBox[{"(", 
                 RowBox[{"intervall", "/", "2"}], ")"}]}]}], ",", 
              RowBox[{"Thread", "[", 
               RowBox[{"y", "->", 
                RowBox[{"yi", "+", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"intervall", "/", "2"}], ")"}], "*", "k2"}]}]}], 
               "]"}]}], "}"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"k4", "=", 
          RowBox[{"phi", "/.", 
           RowBox[{"Flatten", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"t", "->", 
               RowBox[{"ti", "+", "intervall"}]}], ",", 
              RowBox[{"Thread", "[", 
               RowBox[{"y", "->", 
                RowBox[{"yi", "+", 
                 RowBox[{"intervall", "*", "k3"}]}]}], "]"}]}], "}"}], 
            "]"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"yi", "=", 
          RowBox[{"yi", "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"k1", "/", "6"}], "+", 
              RowBox[{"k2", "/", "3"}], "+", 
              RowBox[{"k3", "/", "3"}], "+", 
              RowBox[{"k4", "/", "6"}]}], ")"}], "*", "intervall"}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"out", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"out", ",", "yi"}], "]"}]}], ";"}]}], 
